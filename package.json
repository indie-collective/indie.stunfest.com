{
  "name": "indie-nuxt",
  "version": "1.0.0",
  "description": "Nuxt project for indie.stunfest.com",
  "author": "feoche",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "cross-env STATIC=true nuxt generate",
    "generate:gh": "cross-env STATIC=true DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup --overwrite-local --verbose"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/date-fns": "^1.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "core-js": "^3.30.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "node-sass": "^6.0.1",
    "nuxt": "^2.4.0",
    "nuxt-buefy": "^0.3.2",
    "nuxt-webfontloader": "^1.0.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^10.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "nodemon": "^1.18.9",
    "prettier": "^2.4.1"
  },
  "packageManager": "yarn@3.5.1",
  "engines": {
    "node": "<15"
  }
}
